nav.navbar.navbar-expand-md.navbar-dark.fixed-top.bg-dark
  a.navbar-brand(href='/') Home
  button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarsExampleDefault', aria-controls='navbarsExampleDefault', aria-expanded='false', aria-label='Toggle navigation')
    span.navbar-toggler-icon
  #navbarsExampleDefault.collapse.navbar-collapse
    ul.navbar-nav.mr-auto
      li.nav-item.active
        a.nav-link(href='/')
          | Shop 
          span.sr-only (current)
      li.nav-item.active
        a.nav-link(href='/admin/add-product') Add Product
      li.nav-item
        a.nav-link.disabled(href='#') Disabled
      li.nav-item.dropdown
        a#dropdown01.nav-link.dropdown-toggle(href='#', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false') Dropdown
        .dropdown-menu(aria-labelledby='dropdown01')
          a.dropdown-item(href='#') Action
          a.dropdown-item(href='#') Another action
          a.dropdown-item(href='#') Something else here
    form.form-inline.my-2.my-lg-0
      input.form-control.mr-sm-2(type='text', placeholder='Search', aria-label='Search')
      button.btn.btn-outline-success.my-2.my-sm-0(type='submit') Search
main(role='main')
  // Main jumbotron for a primary marketing message or call to action
  .jumbotron
    .container
      h1.display-3 Welcome to the Phantom Zone
      p
        | This is a template for a simple marketing or informational website. It includes a large callout called a jumbotron and three supporting pieces of content. Use it as a starting point to create something more unique.
      p
        a.btn.btn-primary.btn-lg(href='#', role='button') Learn more &raquo;
  .container
    // Example row of columns
    .row
      each product in prods
        .col-md-4
            h2 #{product.title}
            p
                img.img-thumbnail(src="data:image/jpeg;base64,", alt="")
            p
            a.btn.btn-secondary(href='#', role='button') View details &raquo;
    hr
  // /container
footer.container
  p &copy; Company 2017-2019
